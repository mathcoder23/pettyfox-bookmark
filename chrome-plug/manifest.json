{
  "name": "Pettyfox-Bookmark",
  "version": "1.0",
  "manifest_version": 2,
  "description": "个人私有云书签插件管理",
  "background": {
    "persistent": false,
    "scripts": ["bg.js"]
  },
  "permissions": ["http://*.google.com/", "https://*.google.com/"],
  "chrome_url_overrides": {
    "newtab": "html/tabs.html"
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "icons/128x128.png",
    "default_popup": "popup.html"
  }
}
